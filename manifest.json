{
  "manifest_version": 3,
  "name": "LeetTune",
  "version": "1.0",
  "description": "Leetcode Extension",
   "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [{
        "js": ["content.js"],
        "matches": ["<all_urls>"]
    }],
  "background": {
    "service_worker": "background.js"
  },
  "icons":{
    "48": "assets/icon.png"
  },

  "action": {
        "default_title": "Chrome Addon v3 Starter",
        "default_popup": "pop.html"
    }
} 
